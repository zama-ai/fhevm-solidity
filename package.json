{
  "name": "fhevm",
  "description": "A Solidity library for interacting with the Zama Blockchain",
  "version": "0.1.6",
  "main": "lib/TFHE.sol",
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "scripts": {
    "codegen": "cd ./lib && python ../codegen/codegen.py && npm run prettier && cd ..",
    "lint": "prettier --list-different '**/*.sol'",
    "prettier": "prettier --write '**/*.sol'",
    "test": "hardhat test",
    "build": "hardhat compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zama-ai/fhevm-solidity.git"
  },
  "author": "",
  "license": "BSD-3-Clause-Clear",
  "bugs": {
    "url": "https://github.com/zama-ai/fhevm-solidity/issues"
  },
  "homepage": "https://github.com/zama-ai/fhevm-solidity#readme",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "hardhat": "^2.16.1",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.5"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.2"
  }
}
